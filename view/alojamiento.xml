<odoo>
        <record model="ir.ui.view" id="alojamiento_form_view">
            <field name="name">alojamiento.form</field>
            <field name="model">np.alojamiento</field>
            <field name="arch" type="xml">
                <form string="alojamiento Form">
                    <sheet>
                        <group>
                            <field name="nombre"/>
                            <field name="capacidad"/>
                            <field name="categoria"/>
                            <field name="parquenatural.id"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record model="ir.ui.view" id="alojamiento_tree_view">
            <field name="name">alojamiento.tree</field>
            <field name="model">np.alojamiento</field>
            <field name="arch" type="xml">
                <tree string="alojamiento Tree">
                    <field name="nombre"/>
                    <field name="capacidad"/>
                    <field name="categoria"/>
                    <field name="parquenatural.id"/>                
                </tree>
            </field>
        </record>

        <record model="ir.actions.act_window" id="alojamiento_list_action">
            <field name="name">alojamiento</field>
            <field name="res_model">np.alojamiento</field>
            <field name="view_mode">tree,form</field>
            <field name="help" type="html">
            </field>
        </record>
            <record model="ir.ui.view" id="alojamiento_search_view">
            <field name="name">alojamiento.search</field>
            <field name="model">np.alojamiento</field>
            <field name="arch" type="xml">
                <search>
                    <field name="nombre"/>
                    <filter name="Estrellas" string="Estrellas"
                            domain="[('Estrellas', 'in', ['una', 'dos', 'tres', 'cuatro', 'cinco'])]"/>
                    <filter name="Capacidad" string="Hotel completamente ocupado"
                            domain="[('Capacidad', '&gt;', 50)]"/>
                    <group string="Group By">
                        <filter name="parquenatural" string="Parque nantural"
                                context="{'group_by': 'parquenatural.id'}"/>
                    </group>
                </search>
            </field>
        </record>
        <menuitem id="main_parquenatural_menu" name="parquenatural"/>

        <menuitem id="parquenatural_menu" name="parquenatural"
                  parent="main_parquenatural_menu"/>

        <menuitem id="alojamiento_menu" name="alojamiento"
            parent="parquenatural_menu"
            action="alojamiento_list_action"/>
</odoo>