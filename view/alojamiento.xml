<odoo>
        <record model="ir.ui.view" id="alojamiento_form_view">
            <field name="name">alojamiento.form</field>
            <field name="model">np.alojamiento</field>
            <field name="arch" type="xml">
                <form string="alojamiento Form">
                    <sheet>
                        <group>
                            <field name="nombre"/>
                            <field name="capacidad"/>
                            <field name="categoria"/>
                            <field name="parquenatural.id"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record model="ir.ui.view" id="alojamiento_tree_view">
            <field name="name">alojamiento.tree</field>
            <field name="model">np.alojamiento</field>
            <field name="arch" type="xml">
                <tree string="alojamiento Tree">
                    <field name="nombre"/>
                    <field name="capacidad"/>
                    <field name="categoria"/>
                    <field name="parquenatural.id"/>                
                </tree>
            </field>
        </record>

        <record model="ir.actions.act_window" id="alojamiento_list_action">
            <field name="name">alojamiento</field>
            <field name="res_model">np.alojamiento</field>
            <field name="view_mode">tree,form</field>
            <field name="help" type="html">
            </field>
        </record>
</odoo>