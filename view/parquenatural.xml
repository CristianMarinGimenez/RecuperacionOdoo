<odoo>

        <record model="ir.ui.view" id="parquenatural_form_view">
            <field name="name">parquenatural.form</field>
            <field name="model">np.parquenatural</field>
            <field name="arch" type="xml">
                <form string="Parque naturalForm">
                    <sheet>
                        <group>
                            <field name="nombre"/>
                            <field name="fComienzo"/>
                            <field name="extension" />
                            <field name="comunidad.id"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record model="ir.ui.view" id="parquenatural_tree_view">
            <field name="name">parquenatural.tree</field>
            <field name="model">np.parquenatural</field>
            <field name="arch" type="xml">
                <tree string="Parque naturalTree">
                    <field name="nombre"/>
                    <field name="fComienzo"/>
                    <field name="extension"/>
                    <field name="comunidad.id"/>
                </tree>
            </field>
        </record>

        <record model="ir.ui.view" id="parquenatural_graph_view">
            <field name="name">parquenatural.graph</field>
            <field name="model">np.parquenatural</field>
            <field name="arch" type="xml">
                <graph string="Parque natural">
                    <field name="name"/>
                    <field name="extension" type="measure"/>
                </graph>
            </field>
        </record>

        <record model="ir.actions.act_window" id="parquenatural_list_action">
            <field name="name">Parque natural</field>
            <field name="res_model">np.parquenatural</field>
            <field name="view_mode">tree,form,graph</field>
            <field name="help" type="html"></field>
        </record>
        
        <menuitem id="parquenatural_menu" name="Parque natural
            parent="np_menu"
            action="parquenatural_list_action"/>

</odoo>