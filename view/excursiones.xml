<odoo>
  <record model="ir.ui.view" id="excursiones_form_view">
            <field name="name">excursiones.form</field>
            <field name="model">np.excursiones</field>
            <field name="arch" type="xml">
                <form string="excursiones Form">
                    <sheet>
                        <group>
                            <field name="nombre"/>
                            <field name="tExcursion"/>
                            <field name="DiaSemana"/>
                            <field name="HoraDia"/>
                            <field name="parquenatural.id"/>
                            <field name="alojamiento.id"/>
                            <field name="visitante.id"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record model="ir.ui.view" id="excursiones_tree_view">
            <field name="name">excursiones.tree</field>
            <field name="model">np.excursiones</field>
            <field name="arch" type="xml">
                    <field name="name"/>
                    <field name="tExcursion"/>
                    <field name="DiaSemana"/>
                    <field name="HoraDia"/>
                    <field name="parquenatural.id"/>
                    <field name="alojamiento.id"/>
            </field>
        </record>

        <record model="ir.ui.view" id="excursiones_calendar_view">
            <field name="name">excursiones.calendar</field>
            <field name="model">np.excursiones</field>
            <field name="arch" type="xml">
                <calendar string="excursiones Calendar" date_start="DiaSemana" date_stop="HoraDia" quick_add="False">
                    <field name="name"/>
                    <field name="tExcursion"/>
                    <field name="parquenatural.id"/>
                </calendar>
            </field>
        </record>

        <record model="ir.actions.act_window" id="excursiones_list_action">
            <field name="name">excursiones</field>
            <field name="res_model">np.excursiones</field>
            <field name="view_mode">tree,form</field>
            <field name="help" type="html">
            </field>
        </record>
</odoo>