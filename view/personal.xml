<odoo>
    <!--personal-->
    <record model="ir.ui.view" id="personal_form_view">
        <field name="name">personal.form</field>
        <field name="model">np.personal</field>
        <field name="arch" type="xml">
            <form string="personal Form">
                <sheet>
                    <group colspan="4">
                        <group colspan="2" col="2">
                            <field name="nombre"/>
                            <field name="dni"/>
                            <field name="nSeguridadSocial"/>
                            <field name="direccion"/>
                            <field name="tlfn"/>
                            <field name="tlfnDomicilio"/>
                        </group>
                        <group colspan="2" col="2">
                            <field name="parquenatural.id"/>
                            <field name="salario"/>
                            <field name="nEntrada"/>
                        </group>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record model="ir.ui.view" id="personal_tree_view">
        <field name="name">personal.tree</field>
        <field name="model">np.personal</field>
        <field name="arch" type="xml">
            <tree string="personal Tree">
                <field name="name"/>
                <field name="dni"/>
                <field name="nSeguridadSocial"/>
                <field name="direccion"/>
                <field name="tlfn"/>
                <field name="tlfnDomicilio"/>
                <field name="parquenatural.id"/>
                <field name="salario"/>
                <field name="nEntrada"/>
            </tree>
        </field>
    </record>

    <record model="ir.actions.act_window" id="personal_list_action">
        <field name="name">personal</field>
        <field name="res_model">np.personal</field>
        <field name="view_mode">tree,form</field>
        <field name="help" type="html">
        </field>
    </record>

